<?xml version="1.0"?>
<!DOCTYPE owl [
<!ENTITY foaf "http://xmlns.com/foaf/0.1/">
<!ENTITY dc "http://purl.org/dc/elements/1.1/">
<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
<!ENTITY owl "http://www.w3.org/2002/07/owl#">
<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
<!ENTITY swaml "http://swaml.berlios.de/ns/swaml.owl#">
]>
<rdf:RDF
 xmlns:foaf="http://xmlns.com/foaf/0.1/"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:owl="http://www.w3.org/2002/07/owl#"
 xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
 xmlns:swaml="http://swaml.berlios.de/ns/swaml.owl#"
 xml:base="http://swaml.berlios.de/ns/swaml.owl"
>
<owl:Ontology rdf:about="http://swaml.berlios.de/ns/swaml.owl">
<owl:versionInfo>0.1</owl:versionInfo>
<rdfs:comment>Ontology to describe mail lists</rdfs:comment>
<rdfs:label>SWAML Ontology</rdfs:label>
</owl:Ontology>
<owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
  <rdfs:comment>A mailing list</rdfs:comment>
  <rdfs:label>Mailing List</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
  <rdfs:comment xml:lang="en">A mailing list message</rdfs:comment>
  <rdfs:label>Message</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Suscriptor">
  <rdfs:subClassOf>
    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
    </owl:Class>
  </rdfs:subClassOf>
  <rdfs:comment>A mailing list suscriptor</rdfs:comment>
  <rdfs:label>Suscriptor</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#date">
</owl:Class>
<owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
</owl:Class>
<owl:Class rdf:about="http://www.w3.org/2002/07/owl#Thing">
</owl:Class>
<owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
</owl:Class>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#author">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Suscriptor">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#from" />
  <rdfs:label>author</rdfs:label>
  <rdfs:comment>A person is the author of a message</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#body">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
    </owl:Class>
  </rdfs:range>
  <rdfs:label>body</rdfs:label>
  <rdfs:comment>Message body</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#date">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#date">
    </owl:Class>
  </rdfs:range>
  <rdfs:comment>Message date</rdfs:comment>
  <rdfs:label>date</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#from">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Suscriptor">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#author" />
  <rdfs:comment>A message was sent by a person</rdfs:comment>
  <rdfs:label>from</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#hasSuscriptor">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Suscriptor">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#suscriptorOf" />
  <rdfs:comment>A mailing list has a suscriptor</rdfs:comment>
  <rdfs:label>hasSuscriptor</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#id">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
    </owl:Class>
  </rdfs:range>
  <rdfs:comment>message unique id</rdfs:comment>
  <rdfs:label>id</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#nextByDate">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#previousByDate" />
  <rdfs:comment xml:lang="en">Next message sent in mailing list</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#post">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
    </owl:Class>
  </rdfs:range>
  <rdfs:label>post</rdfs:label>
  <rdfs:comment>Mailign list post address. Many people prefer to use swaml:post_sha1sum</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#post_sha1sum">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
    </owl:Class>
  </rdfs:range>
  <rdfs:comment xml:lang="en">Applying the SHA1 mathematical functional to a 'mailto:' identifier (URI) for the mailing list post address</rdfs:comment>
  <rdfs:label>post_sha1sum</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#previousByDate">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#nextByDate" />
  <rdfs:label>previousByDate</rdfs:label>
  <rdfs:comment>Previous message by date in mailign list</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#sentIn">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#sentMail" />
  <rdfs:comment>Message sent it in a mailing list</rdfs:comment>
  <rdfs:label>sentIn</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#sentMail">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#sentIn" />
  <rdfs:comment>a mailing list have a sent mail</rdfs:comment>
  <rdfs:label>sentMail</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#subject">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#string">
    </owl:Class>
  </rdfs:range>
  <rdfs:label>subject</rdfs:label>
  <rdfs:comment>Message subject</rdfs:comment>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://swaml.berlios.de/ns/swaml.owl#suscriptorOf">
  <rdfs:domain>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Suscriptor">
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
    </owl:Class>
  </rdfs:range>
  <owl:inverseOf rdf:resource="http://swaml.berlios.de/ns/swaml.owl#hasSuscriptor" />
  <rdfs:label>suscriptorOf</rdfs:label>
  <rdfs:comment>Suscriptor of a mailing list</rdfs:comment>
</owl:ObjectProperty>



<owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
  <owl:disjointWith>
  <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
  </owl:Class>
  </owl:disjointWith>
</owl:Class>
<owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
  <owl:disjointWith>
  <owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#Message">
  </owl:Class>
  </owl:disjointWith>
</owl:Class>
<owl:Class rdf:about="http://swaml.berlios.de/ns/swaml.owl#MailingList">
  <owl:disjointWith>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
  </owl:Class>
  </owl:disjointWith>
</owl:Class>
</rdf:RDF>
